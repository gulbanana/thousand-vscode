{
	"name": "thousand",
	"displayName": "Thousand",
	"description": "Language support for thousand words (.1000 files) in Visual Studio Code.",
	"version": "0.0.1",
	"publisher": "gulbanana",
	"repository": {
		"type": "git",
		 "url": "https://github.com/gulbanana/thousand-vscode"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:thousand.preview"
	],
	"contributes": {
		"commands": [
			{
				"command": "thousand.preview",
				"title": "Thousand: Open Preview to the Side"
			}
		]
	}
}
